{
    "name": "inlinestudio/mailconnectors",
    "description": "A library with connectors for api based email sending on Google and O365",
    "license": "MIT",
    "keywords": [
        "laravel",
        "mail",
        "library"
    ],
    "authors": [
        {
            "name": "Inline Studio",
            "email": "office@inlinestudio.hu",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/inlinestudio/laravel-api-mailer-connectors",
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0 || ^11.0 || ^12.0",
        "microsoft/microsoft-graph": "^2.56",
        "spatie/laravel-package-tools": "^1.13.0"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.1",
        "ergebnis/composer-normalize": "^2.49",
        "fakerphp/faker": "^1.24",
        "laravel/pint": "^1.27",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.8",
        "orchestra/canvas": "^10.0",
        "orchestra/testbench": "^10.9",
        "pestphp/pest": "^4.3",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-livewire": "^4.1",
        "pestphp/pest-plugin-type-coverage": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^12.5",
        "rector/rector": "^2.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "InlineStudio\\MailConnectors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InlineStudio\\MailConnectors\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "aliases": {
                "MailConnector": "InlineStudio\\MailConnectors\\Facades\\MailConnector"
            },
            "providers": [
                "InlineStudio\\MailConnectors\\MailConnectorServiceProvider"
            ]
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    }
}
